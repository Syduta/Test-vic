{% extends 'base.html.twig' %}
{% block title %} Feacell's Drawings {% endblock %}
{% block body %}
    <h1>Feacell's Drawings</h1>
    {% for drawing in drawings %}
        <h1>{{ drawing.title }}</h1>
        <img src="{{ drawing.image }}" alt="{{ drawing.title }}">
        <p>{{ drawing.medium }}</p>
    {% endfor %}
{% endblock %}